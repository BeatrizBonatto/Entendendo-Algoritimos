#include <stdio.h>

int soma(int lista[], int tamanho) {
    int total = 0;
    for(int i = 0; i < tamanho; i++)
    {
        total += lista[i];
    }
    return total;
}

int main() {
    int numeros[] = {1, 2, 3, 4};
    int tamanho = sizeof(numeros) / sizeof(numeros[0]);
    
    printf("%d\n", soma(numeros, tamanho));
    
    return 0;
}
